{% extends 'base.html.twig' %}

{% block title %}Hello HomepageController!{% endblock %}

{% block body %}

    <div class="container">
        {% for log in data %}
            <code>
                <p >
                    $>
                    <span class="red">{{log['level']}}</span>
                    <span class="violet">{{log['client_ip']}}</span>
                    <span class="orange">{{log['message']}}</span>
                </p>
            </code>
        {% endfor %}
    </div>

{% endblock %}
